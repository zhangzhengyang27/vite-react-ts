    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.1",
